# Copy this file to .env and adjust values for your environment.

# Full API URL (include protocol, host, port, and trailing /api)
# The frontend defaults to matching the page origin if this is omitted.
REACT_APP_API_URL=https://your.server.hostname:3101/api

# Optional granular overrides (used when REACT_APP_API_URL is unset)
# REACT_APP_BACKEND_PROTOCOL=https:
# REACT_APP_BACKEND_HOST=your.server.hostname
# REACT_APP_BACKEND_PORT=3101

# Port for the React dev server (only used locally with npm start)
PORT=3102
